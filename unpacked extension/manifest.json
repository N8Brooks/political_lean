 {
  "manifest_version": 2,
  "name": "Political Lean",
  "version": "1.3.3.7",
  "permissions": ["activeTab", "https://us-central1-braided-upgrade-262021.cloudfunctions.net/function-1"],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "description": "Uses machine learning to detect political lean of articles.",
  "icons": {"128": "icon.png"},
  "browser_action": {
   "default_title": "Detect Left/Right Lean!",
   "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "notificationIcon.png"
  ]
}

